{
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "vite build --outDir docs",
    "test": "wtr src/**/*.test.js --node-resolve",
    "exports": "exports src/components.js src/**/*.jsx --exports --types --main components --minify",
    "prepublishOnly": "npm run test && npm run exports",
    "create-component": "scaff template/component-jsx src"
  },
  "dependencies": {
    "atomico": "^1.34.5",
    "brotli-cli": "^1.0.3",
    "mitt": "^3.0.0",
    "postcss": "^8.4.7"
  },
  "devDependencies": {
    "@atomico/exports": "^0.22.2",
    "@atomico/scaffold": "^0.2.0",
    "@csstools/postcss-sass": "^5.0.0",
    "@uppercod/vite-meta-url": "^1.5.0",
    "@uppercod/vite-meta-url-load-css": "^1.2.0",
    "classnames": "^2.3.1",
    "open-props": "^1.3.8",
    "postcss-custom-media": "^8.0.0",
    "postcss-jit-props": "^1.0.4",
    "tailwindcss": "^3.0.7",
    "tsc": "^2.0.4",
    "typescript": "^4.5.5",
    "vite": "latest"
  },
  "module": "./dist/components.js",
  "exports": {
    ".": "./dist/components.js",
    "./seam-button": "./dist/seam-button/seam-button.js",
    "./seam-check-icon": "./dist/seam-check-icon/seam-check-icon.js",
    "./seam-loading-icon": "./dist/seam-loading-icon/seam-loading-icon.js",
    "./seam-x-icon": "./dist/seam-x-icon/seam-x-icon.js"
  },
  "types": "types/components.d.ts",
  "typesVersions": {
    "*": {
      "components": [
        "./types/components.d.ts"
      ],
      "seam-button": [
        "./types/seam-button/seam-button.d.ts"
      ],
      "seam-loading-icon": [
        "./types/seam-loading-icon/seam-loading-icon.d.ts"
      ],
      "seam-check-icon": [
        "./types/seam-check-icon/seam-check-icon.d.ts"
      ],
      "seam-x-icon": [
        "./types/seam-x-icon/seam-x-icon.d.ts"
      ]
    }
  }
}
